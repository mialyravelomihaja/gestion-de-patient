package patient.bean;

public class patient {
	private static int id;
	private static String nomPat;
	private static String prenomPat;
	private static String agePat;
	private static String telPat;
	private static String date;
	private static String resultat;
	
	
	public patient(String nomPat, String prenomPat, String agePat, String telPat, String date, String resultat) {
		super();
		patient.nomPat = nomPat;
		this.prenomPat = prenomPat;
		this.agePat = agePat;
		this.telPat = telPat;
		this.date = date;
		this.resultat = resultat;
	}
	public patient(int id, String nomPat, String prenomPat, String agePat, String telPat, String date,
			String resultat) {
		super();
		this.id = id;
		this.nomPat = nomPat;
		this.prenomPat = prenomPat;
		this.agePat = agePat;
		this.telPat = telPat;
		this.date = date;
		this.resultat = resultat;
	}
	public static int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public static String getNomPat() {
		return nomPat;
	}
	public void setNomPat(String nomPat) {
		this.nomPat = nomPat;
	}
	public static String getPrenomPat() {
		return prenomPat;
	}
	public void setPrenomPat(String prenomPat) {
		this.prenomPat = prenomPat;
	}
	public static String getAgePat() {
		return agePat;
	}
	public void setAgePat(String agePat) {
		this.agePat = agePat;
	}
	public static String getTelPat() {
		return telPat;
	}
	public void setTelPat(String telPat) {
		this.telPat = telPat;
	}
	public static String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public static String getResultat() {
		return resultat;
	}
	public void setResultat(String resultat) {
		this.resultat = resultat;
	}
	
	

}
